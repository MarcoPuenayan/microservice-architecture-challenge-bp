{
  "name": "mf",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": " ng serve -c local ",
    "start:dev": "ng serve -c dev",
    "start:cert": "ng serve -c cert",
    "start:prod": "ng serve -c production",
    "build": "ng build -c local",
    "build:local": "ng build -c local",
    "build:prod": "ng build -c production",
    "build:cert": "ng build -c cert",
    "build:dev": "ng build -c dev",
    "build:development": "ng build -c dev && npm run post-build",
    "build:gzip": "gzipper c ./dist/mfa-transactions  --include js,css,html --output-file-format [filename].[ext].gz ./dist/mfa-transactions/ && rm ./dist/mfa-transactions/index.html ",
    "post-build-gz": "mv dist/mfa-transactions/index.html.gz ./dist",
    "build:staging": "ng build -c cert && npm run post-build",
    "build:production": "ng build -c production && npm run post-build",
    "post-build": "mv dist/mfa-transactions/index.html ./dist",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "coverage": "ng test --coverage",
    "test:deploy": "ng test --watch=false --coverage",
    "watch": "ng build --watch --configuration development",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.html .",
    "lint:fix": "npx lint \"src/**/*.{js,jsx,ts,tsx,html}\" --fix ",
    "prettier": "npx pretty-quick --staged && npx lint \"src/**/*.{js,jsx,ts,tsx,html}\" --fix && ng test",
    "docs:build": "npx compodoc -p tsconfig.doc.json",
    "docs:build-and-serve": "npx compodoc -p tsconfig.doc.json -c compodocrc.json -s",
    "docs:serve": "npx compodoc -p tsconfig.doc.json -c compodocrc.json -s",
    "docs:watch": "npx compodoc -p tsconfig.doc.json -c compodocrc.json -s -w"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^17.3.4",
    "@angular/compiler": "^17.3.4",
    "@angular/core": "^17.3.4",
    "@angular/elements": "^17.3.4",
    "@angular/forms": "^17.3.4",
    "@angular/platform-browser": "^17.3.4",
    "@angular/platform-browser-dynamic": "^17.3.4",
    "@angular/router": "^17.3.4",
    "@pichincha/angular-sdk": "^1.8.12",
    "@pichincha/bb-commons": "^1.1.11",
    "@pichincha/ds-angular": "2.21.2",
    "@pichincha/ds-core": "2.21.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@softarc/native-federation": "^2.0.8",
    "@angular-architects/module-federation": "^17.0.8",
    "@angular-builders/jest": "17.0.3",
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.0",
    "@angular/compiler-cli": "^17.3.4",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@compodoc/compodoc": "^1.1.19",
    "@pichincha/eslint-plugin-angular-architecture": "^1.3.6",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "concat": "^1.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-system": "^2.2.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "gzipper": "^7.2.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "ngx-build-plus": "^17.0.0",
    "path": "^0.12.7",
    "prettier": "^2.8.6",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "typescript": "~5.4.5",
    "html-loader": "^5.0.0"
  }
}
