{
  "name": "shell",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local",
    "start:dev": "ng serve -c dev",
    "start:cert": "ng serve -c cert",
    "start:prod": "ng serve -c production",
    "build": "ng build -c local",
    "build:local": "ng build -c local",
    "build:prod": "ng build -c production",
    "build:cert": "ng build -c cert",
    "build:dev": "ng build -c dev",
    "build:development": "ng build -c dev && npm run post-build",
    "build:staging": "ng build -c cert && npm run post-build",
    "build:production": "ng build -c production && npm run post-build",
    "post-build": "mv dist/shell-financial-management/index.html ./dist",
    "build:gzip": "gzipper c ./dist/shell-financial-management --include js,css,html --output-file-format [filename].[ext].gz ./dist/shell-financial-management/ && rm ./dist/shell-financial-management/index.html ",
    "post-build-gz": "mv dist/shell-financial-management/index.html.gz ./dist",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "test:deploy": "ng test --watch=false --coverage",
    "coverage": "ng test --coverage",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.html .",
    "lint:fix": "eslint --ext .ts,.html . --fix",
    "prettier": "npx pretty-quick --staged && eslint --ext .ts,.html --fix",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation-tools": "~15.0.3",
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@pichincha/angular-sdk": "2.1.0",
    "@pichincha/ds-angular": "3.0.0",
    "@pichincha/ds-core": "3.0.0",
    "rxjs": "~7.8.1",
    "tslib": "~2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "~17.0.8",
    "@angular-builders/jest": "18.0.0",
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "^18.4.2",
    "@angular-eslint/eslint-plugin": "^18.4.2",
    "@angular-eslint/eslint-plugin-template": "^18.4.2",
    "@angular-eslint/schematics": "^18.4.2",
    "@angular-eslint/template-parser": "^18.4.2",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@commitlint/cli": "^18.2.13",
    "@commitlint/config-conventional": "^18.6.3",
    "@compodoc/compodoc": "^1.1.19",
    "@pichincha/eslint-plugin-angular-architecture": "^1.3.6",
    "@pichincha/frontend-cli": "1.11.0",
    "@softarc/native-federation": "^2.0.8",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "concat": "^1.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "gzipper": "^7.2.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "ngx-build-plus": "^17.0.0",
    "prettier": "^2.8.6",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "typescript": "~5.4.5"
  }
}
